{
  "name": "vue3-scaffold",
  "version": "1.0.0",
  "description": "vue3简易脚手架",
  "scripts": {
    "dev": "vite",
    "build:staging": "cross-env CURRENT_ENV=staging run-p type-check build-only",
    "build:prod": "cross-env CURRENT_ENV=production run-p type-check build-only",
    "type-check": "vue-tsc --noEmit",
    "build-only": "vite build",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "pub:staging": "cross-env CURRENT_ENV=staging node ./scripts/publish.ts",
    "pub:prod": "cross-env CURRENT_ENV=production node ./scripts/publish.ts"
  },
  "author": "Artoriasless",
  "license": "MIT",
  "homepage": "https://github.com/artoriasless/vue3-scaffold#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artoriasless/vue3-scaffold.git"
  },
  "bugs": {
    "url": "https://github.com/artoriasless/vue3-scaffold/issues"
  },
  "dependencies": {
    "@element-plus/icons-vue": "2.0.10",
    "@revolist/vue3-datagrid": "3.2.17",
    "axios": "1.3.2",
    "crypto-js": "4.1.1",
    "dayjs": "1.11.7",
    "element-plus": "2.2.29",
    "js-cookie": "3.0.5",
    "lodash": "4.17.21",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "uuid": "9.0.0",
    "vue": "3.2.47",
    "vue-router": "4.0.13",
    "vuex": "4.0.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.1.0",
    "@types/fs-extra": "11.0.1",
    "@types/lodash": "4.14.191",
    "@types/node": "18.13.0",
    "@vitejs/plugin-legacy": "4.1.0",
    "@vitejs/plugin-vue": "3.0.1",
    "@vitejs/plugin-vue-jsx": "3.0.0",
    "@vue/eslint-config-typescript": "11.0.0",
    "@vue/tsconfig": "0.1.3",
    "ali-oss": "6.17.1",
    "cross-env": "7.0.3",
    "eslint": "8.5.0",
    "eslint-plugin-vue": "9.0.0",
    "fs-extra": "11.1.1",
    "sass": "1.58.0",
    "sass-loader": "13.2.0",
    "svg-sprite-loader": "6.0.11",
    "terser": "5.18.2",
    "typescript": "4.9.5",
    "vite": "4.1.1",
    "vite-plugin-eslint": "1.8.1",
    "vue-tsc": "0.38.8"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
